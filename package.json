{
  "name": "asymptotic-analyzer",
  "version": "0.0.8",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "check": "concurrently npm:check:*",
    "check:types": "npm run check:types --workspaces",
    "check:format": "prettier . --check",
    "check:lint": "eslint .",
    "dev": "concurrently \"npm:dev --workspace=client\" \"npm:dev --workspace=server\""
  },
  "bin": {
    "analyze": "bin/analyze.js"
  },
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "dependencies": {
    "common-tags": "^1.8.2",
    "esbuild": "^0.15.7",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.10"
  }
}
